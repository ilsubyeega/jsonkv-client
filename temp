const client = ...;
client.bulk_listen(i => {
  i.listen < Temp > ('temp', msg => i = 2);
  i.listen < Overall > ('overall', msg => i = 3);

  i.on('connect', () => console.log("Connected"))
  i.on('ready', () => console.log("Authenticated"))
  i.on('reconnect', () => console.log("Reconnected"))
  i.on('disconnect', () => console.log("Disconnected"))
  i.on('error', (e) => console.log("Error"))
  
  i.option({
    mode: "full"
  })

  i.connect()
  i.close()
})
